function formatValidity(days) {
  if (days === null || days === void 0) return "N/A";
  const d = Number(days);
  if (!Number.isFinite(d) || d <= 0) return "Per task";
  if (d % 365 === 0) {
    const years = d / 365;
    return years === 1 ? "12 months" : `${years} years`;
  }
  if (d % 30 === 0) {
    const months = d / 30;
    return months === 1 ? "1 month" : `${months} months`;
  }
  return `${d} days`;
}

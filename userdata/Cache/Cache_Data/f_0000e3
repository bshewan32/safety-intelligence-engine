function AssignWorkerModal({
  clientId,
  sites,
  onClose,
  onSuccess
}) {
  _s2();
  const [workers, setWorkers] = useState([]);
  const [roles, setRoles] = useState([]);
  const [selectedWorkerId, setSelectedWorkerId] = useState("");
  const [selectedRoleId, setSelectedRoleId] = useState("");
  const [selectedSiteId, setSelectedSiteId] = useState("");
  const [isPrimary, setIsPrimary] = useState(false);
  const [loading, setLoading] = useState(false);
  useEffect(() => {
    loadData();
  }, []);
  const loadData = async () => {
    try {
      const [workersData, rolesData] = await Promise.all(
        [
          window.api.listWorkers(),
          window.api.listRoles()
        ]
      );
      const availableWorkers = (workersData || []).filter((worker) => {
        if (!worker.roles || worker.roles.length === 0) {
          return true;
        }
        const hasOtherClientAssignment = worker.roles.some((role) => {
          const isOtherClient = role.clientId && role.clientId !== clientId;
          const isActive = !role.endAt || new Date(role.endAt) > /* @__PURE__ */ new Date();
          return isOtherClient && isActive;
        });
        return !hasOtherClientAssignment;
      });
      setWorkers(availableWorkers);
      setRoles(rolesData || []);
    } catch (error) {
      console.error("Failed to load data:", error);
    }
  };
  const handleAssign = async () => {
    if (!selectedWorkerId || !selectedRoleId) {
      alert("Please select both a worker and a role");
      return;
    }
    setLoading(true);
    try {
      await window.api.addWorkerRole({
        workerId: selectedWorkerId,
        roleId: selectedRoleId,
        clientId,
        siteId: selectedSiteId || void 0,
        isPrimary
      });
      onSuccess();
    } catch (error) {
      console.error("Failed to assign worker:", error);
      alert("Failed to assign worker");
    } finally {
      setLoading(false);
    }
  };
